{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "src",
    "skipLibCheck": true,
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": false,
    "allowImportingTsExtensions": false
  },
  "include": ["src"]
}
